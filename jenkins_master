pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                script {
                    def data = readFile(file: 'data.txt')
                    def line = data.readLines()

                    echo 'Start Trigger to send name and age to slave'
                    build job: 'homework_slave', parameters: [string(name: 'NAME', value: lines.get(0)), string(name: 'Ages', value: lines.get(1))]
                }
            }
        }
    }
}